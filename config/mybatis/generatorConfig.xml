<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<context id="bookStoreTables" targetRuntime="MyBatis3" >

		<commentGenerator>
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/bookStore" userId="root"
			password="hr12345@SQL">
		</jdbcConnection>
		
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.bookStore.model"
			targetProject=".\src">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

 		<sqlMapGenerator targetPackage="com.bookStore.mapper"
			targetProject=".\src">
			<property name="enableSubPackages" value="true" />	
		</sqlMapGenerator> 

	<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.bookStore.mapper" targetProject=".\src">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator> 

		
		<table tableName="user"></table>
		<table tableName="book"></table>
		<table tableName="book_info"></table>
		

	</context>
</generatorConfiguration>